    </servlet>
    <servlet>
        <servlet-name>konacnaNarudzbinaServlet</servlet-name>
        <servlet-class>servlets.konacnaNarudzbinaServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MasterServlet</servlet-name>
        <url-pattern>/MasterServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PrijavaServlet</servlet-name>
        <url-pattern>/PrijavaServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>PromenaLozinkeServlet</servlet-name>
        <url-pattern>/PromenaLozinkeServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>FormaServlet</servlet-name>
        <url-pattern>/FormaServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SefKuhinjeServlet</servlet-name>
        <url-pattern>/SefKuhinjeServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SefPoslovniceServlet</servlet-name>
        <url-pattern>/SefPoslovniceServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>MenadzerServlet</servlet-name>
        <url-pattern>/MenadzerServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Admin</servlet-name>
        <url-pattern>/Admin</url-pattern>
    </servlet-mapping>
