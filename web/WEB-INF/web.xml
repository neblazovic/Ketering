        <servlet-name>SlatkiServlet</servlet-name>
        <url-pattern>/SlatkiServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>konacnaNarudzbinaServlet</servlet-name>
        <url-pattern>/konacnaNarudzbinaServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
    <resource-ref>
        <description>DB Connection</description>
        <res-ref-name>ketering</res-ref-name>
        <res-type>javax.sql.ConnectionPoolDataSource</res-type>
        <res-auth>Container</res-auth>
    </resource-ref>
