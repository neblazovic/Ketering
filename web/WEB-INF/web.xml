        <servlet-class>servlets.Logout</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ReklamacijeServlet</servlet-name>
        <servlet-class>servlets.ReklamacijeServlet</servlet-class>
    </servlet>
    <servlet>
