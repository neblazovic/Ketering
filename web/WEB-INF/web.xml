<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <servlet>
        <servlet-name>MasterServlet</servlet-name>
        <servlet-class>servlets.MasterServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PrijavaServlet</servlet-name>
        <servlet-class>servlets.PrijavaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>PromenaLozinkeServlet</servlet-name>
        <servlet-class>servlets.PromenaLozinkeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>FormaServlet</servlet-name>
        <servlet-class>servlets.FormaServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SefKuhinjeServlet</servlet-name>
        <servlet-class>servlets.SefKuhinjeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SefPoslovniceServlet</servlet-name>
        <servlet-class>servlets.SefPoslovniceServlet</servlet-class>
